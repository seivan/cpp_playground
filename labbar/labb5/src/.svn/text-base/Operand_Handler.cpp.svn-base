#include "Operand_Handler.h"
using namespace std;

Operand_Handler::Operand_Handler() {}

Operand_Handler::~Operand_Handler() {}

bool Operand_Handler::is_of_type(const std::string& token) {
	if (isdigit(token[0])) {
		return true;
	} else {
		return false;
	}
}

